[General]
structure=@MENUD_STRUCTURE@
trans=@MENUD_TRANS@
ea09=true

[Network]
server=127.0.0.1:12080

[rvw2prg]
title=Programme
root=/home/robotino/programs
filter=*.rvw2
command=/usr/local/RobotinoView2/bin/robview2_interpreter
;command=E:/build/RobotinoView/trunk/target/debug/robview2_interpreter.exe
param=--set
param1=Robotino:ipAddress=127.0.0.1
param2=-f

[DEMO1]
title=Kreis
command=/usr/local/OpenRobotinoAPI/1/examples/c++/circle/example_circle
param=192.168.1.91

[DEMO2]
title=Vorwärts
command=/usr/local/OpenRobotinoAPI/1/examples/c++/forward/example_forward
param=127.0.0.1

[DEMO3]
title=Viereck
command=/usr/local/OpenRobotinoAPI/1/examples/c++/rect/example_rect
param=127.0.0.1

[DEMO4]
title=Erkunden
command=/usr/local/OpenRobotinoAPI/1/examples/c++/roaming/example_roaming
param=127.0.0.1

[DEMO5]
title=Wandverfolgung
command=/usr/local/OpenRobotinoAPI/1/examples/c++/wallfollow/example_wallfollow
param=127.0.0.1

[DEMO6]
title=Ind.Sensor AI0
command=/usr/local/OpenRobotinoAPI/1/examples/c++/inductive/example_inductive
param=127.0.0.1

[DEMO7]
title=Linie folgen
command=/usr/local/OpenRobotinoAPI/1/examples/c++/linefollow/example_linefollow
param=127.0.0.1

[DEMO8]
title=Manipulator
command=/usr/local/OpenRobotinoAPI/1/examples/c++/manipulator/example_manipulator
param=127.0.0.1

;[MYPRG1]
;title=My prg 1
;command=/home/robotino/myprg1
;param=127.0.0.1

;[MYPRG2]
;title=My prg 2
;command=/home/robotino/myprg2
;param=127.0.0.1