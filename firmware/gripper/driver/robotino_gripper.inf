;--------------------------------------------------------
;ï¿½@qshot.Inf
;--------------------------------------------------------

[Version]
LayoutFile=layout.inf
Signature="$CHICAGO$"
Class=Ports
ClassGuid={4D36E978-E325-11CE-BFC1-08002BE10318}
Provider=%REC%
DriverVer=04/14/2010,1.0.0.0

[DestinationDirs]
DefaultDestDir=12

[Manufacturer]
%MfgName%=Models,NTamd64

[Models.NTamd64]
%USB.PnP%=ComPort, USB\VID_1E29&PID_040F


[ClassInstall]
AddReg=PortsClass.AddReg

[PortsClass.AddReg]
HKR,,,,%PortsClassName%

[ClassInstall32.NT]
AddReg=PortsClass.NT.AddReg

[PortsClass.NT.AddReg]
HKR,,,,%PortsClassName%
HKR,,Icon,,"-23"
HKR,,Installer32,,"MsPorts.Dll,PortsClassInstaller"

[ControlFlags]
ExcludeFromSelect=*

[Models]
%USB.PnP%=ComPort, USB\VID_1E29&PID_040F

[ComPort.NT]
include=mdmcpq.inf
CopyFiles=ComPort.Copy
AddReg=ComPort.AddReg, ComPort.NT.AddReg

[ComPort.NT.HW]
include=mdmcpq.inf
AddReg=ComPort.NT.HW.AddReg

[ComPort.NT.Services]
include=mdmcpq.inf
AddService = usbser, 0x00000002, Serial_Service_Inst,
Serial_EventLog_Inst

[ComPort.NT.HW.AddReg]

; -------------- USBSerial Port Driver install sections
[Serial_Service_Inst]
DisplayName    = %Serial.SVCDESC%
ServiceType   = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\usbser.sys
LoadOrderGroup = Extended base

[Serial_EventLog_Inst]
AddReg = Serial_EventLog_AddReg

[Serial_EventLog_AddReg]
HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%
%SystemRoot%%\System32\drivers\usbser.sys"
HKR,,TypesSupported,0x00010001,7

; COM sections
;----------------------------------------------------------
[ComPort.Copy]
usbser.sys,,,0x20

[ComPort.AddReg]
HKR,,PortSubClass,1,01

[ComPort.NT.Copy]
CopyFiles=ComPort.Copy

[ComPort.NT.AddReg]
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[Strings]
MfgName="Robotics Equipment Corporation GmbH"
REC="Robotics Equipment Corporation GmbH"
DiskName_Desc="Robotino Gripper driver"
PortsClassName = "Ports (COM & LPT)"
Serial.SVCDESC = "REC USB Serial Port Driver"
USB.PnP="REC Robotino Gripper USB Device Driver"
